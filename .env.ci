# CI/CD Environment variables for deployment testing
# These variables configure the deployment to use self-signed certificates for CI testing

# SSL Certificate paths for CI testing (using self-signed certificates)
SSL_CERTS_PATH=./ssl_certs

# Database configuration
# DATABASE_NAME=junqo
# DATABASE_USER=junqo
# DATABASE_PORT=5432
# DATABASE_SHM_SIZE=128mb
DATABASE_PASSWORD_FILE=./db_password_exemple.conf

# Backend configuration
BACK_ENV_FILE=./junqo_back/.env.ci

# Frontend configuration
FRONT_ENV_FILE=./junqo_front/config/.env.ci
API_URL=http://localhost/api/v1

# Flutter configuration
# FLUTTER_VERSION=3.35.3

# Monitoring configuration
# ADMINER_DESIGN=pepa-linha-dark
GRAFANA_PASSWORD_FILE=./grafana_password.conf
